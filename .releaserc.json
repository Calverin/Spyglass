{
    "plugins": [
        "semantic-release-gitmoji",
        "@semantic-release/changelog",
        "@semantic-release/npm",
        "@semantic-release/github",
        [
            "@semantic-release/git",
            {
                "message": ":bookmark: v${nextRelease.version}\n\n[skip ci]\n\nhttps://github.com/SPGoding/mcfunction-language-server/releases/tag/${nextRelease.gitTag}"
            }
        ]
    ]
}
